LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.STD_LOGIC_ARITH.ALL;
USE IEEE.STD_LOGIC_UNSIGNED.ALL;

ENTITY test IS
	PORT(IR_Sensor: IN STD_LOGIC;
		  LED: OUT STD_LOGIC);
END test;

ARCHITECTURE Behavioral OF test IS
BEGIN 
   PROCESS(IR_Sensor)
	BEGIN
		IF IR_Sensor= '1' THEN LED<='1';
		ELSE LED<='0'
		END IF;
	END PROCESS;
END Behavioral;